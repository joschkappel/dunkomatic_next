{
    "name": "laravel/laravel",
    "type": "project",
    "description": "dunkomatic manage basketball seasons",
    "keywords": [
        "framework",
        "laravel",
        "dunkomatic"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "bensampo/laravel-enum": "^3.0",
        "fakerphp/faker": "^1.9.1",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "v2.0.4",
        "graylog2/gelf-php": "^1.5",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^v8.78",
        "laravel/slack-notification-channel": "^2.3",
        "laravel/telescope": "^4.3",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "laravelcollective/html": "^6.1",
        "maatwebsite/excel": "^3.1",
        "mockery/mockery": "^1.3.1",
        "mpdf/mpdf": "^8.0",
        "owen-it/laravel-auditing": "^10.0",
        "phpunit/phpunit": "^9.3",
        "predis/predis": "^1.0",
        "rappasoft/laravel-authentication-log": "^1.2",
        "silber/bouncer": "^v1.0.0-rc.9",
        "spatie/cpu-load-health-check": "^1.0",
        "spatie/icalendar-generator": "2.3.0",
        "spatie/laravel-health": "^1.7",
        "spatie/temporary-directory": "^2.0",
        "stuk/jszip": "^3.0",
        "symfony/http-kernel": "^5.1.5",
        "torann/geoip": "^3.0",
        "yajra/laravel-datatables-oracle": "^9.10"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.12",
        "facade/ignition": "^2.3.6",
        "laravel/dusk": "^6.9",
        "nunomaduro/collision": "^5.0",
        "nunomaduro/larastan": "^1.0",
        "roquie/laravel-dusk-select2": "^2.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },

    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
