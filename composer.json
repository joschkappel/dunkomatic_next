{
    "name": "laravel/laravel",
    "type": "project",
    "description": "dunkomatic manage basketball seasons",
    "keywords": [
        "framework",
        "laravel",
        "dunkomatic"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "bensampo/laravel-enum": "^5.1",
        "fakerphp/faker": "^1.9.1",
        "graylog2/gelf-php": "^1.5",
        "guzzlehttp/guzzle": "^7.2",
        "itsgoingd/clockwork": "^5.1",
        "laravel/framework": "^9.2",
        "laravel/slack-notification-channel": "^2.4",
        "laravel/socialite": "^5.5",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^3.0",
        "laravelcollective/html": "^6.1",
        "league/flysystem-aws-s3-v3": "^3.0",
        "maatwebsite/excel": "^3.1",
        "mews/captcha": "^3.2",
        "mockery/mockery": "^1.3.1",
        "mpdf/mpdf": "^8.0",
        "owen-it/laravel-auditing": "^12.0",
        "phpunit/phpunit": "^9.5.10",
        "predis/predis": "^1.0",
        "rappasoft/laravel-authentication-log": "^2.0",
        "silber/bouncer": "^v1.0.0-rc.9",
        "spatie/cpu-load-health-check": "^1.0",
        "spatie/icalendar-generator": "2.3.0",
        "spatie/laravel-health": "^1.7",
        "spatie/temporary-directory": "^2.0",
        "stuk/jszip": "^3.0",
        "symfony/http-kernel": "^6.0",
        "torann/geoip": "^3.0",
        "yajra/laravel-datatables-oracle": "^9.10"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.12",
        "laravel/dusk": "^6.9",
        "laravel/telescope": "^4.7",
        "nunomaduro/collision": "^6.1",
        "nunomaduro/larastan": "^1.0",
        "roquie/laravel-dusk-select2": "^2.1",
        "spatie/laravel-ignition": "^1.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },

    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
