{
    "name": "laravel/laravel",
    "type": "project",
    "description": "dunkomatic manage basketball seasons",
    "keywords": [
        "framework",
        "laravel",
        "dunkomatic"
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/laravel-shift/laravel-er-diagram-generator"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "asantibanez/livewire-charts": "^2.4",
        "bensampo/laravel-enum": "*",
        "fakerphp/faker": "^1.9.1",
        "graylog2/gelf-php": "^1.5",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^10",
        "laravel/horizon": "^5.18",
        "laravel/reverb": "@beta",
        "laravel/slack-notification-channel": "^2.4",
        "laravel/socialite": "^5.5",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^4.0",
        "laravelcollective/html": "^6.4",
        "league/flysystem-aws-s3-v3": "^3.0",
        "livewire/livewire": "^2.10",
        "maatwebsite/excel": "^3.1",
        "mews/captcha": "^3.2",
        "mockery/mockery": "^1.3.1",
        "mpdf/mpdf": "^8.0",
        "opcodesio/log-viewer": "^3.1",
        "owen-it/laravel-auditing": "^13.6",
        "phpunit/phpunit": "^9.5.10",
        "predis/predis": "^2",
        "psr/simple-cache": "^2.0",
        "rappasoft/laravel-authentication-log": "*",
        "silber/bouncer": "^v1.0.0-rc.9",
        "spatie/cpu-load-health-check": "^1.0",
        "spatie/icalendar-generator": "2.3.0",
        "spatie/laravel-db-snapshots": "^2.6",
        "spatie/laravel-health": "^1.7",
        "spatie/laravel-html": "*",
        "spatie/temporary-directory": "^2.0",
        "stuk/jszip": "^3.0",
        "symfony/http-kernel": "^6.0",
        "torann/geoip": "^3.0",
        "yajra/laravel-datatables-oracle": "*"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.12",
        "beyondcode/laravel-er-diagram-generator": "dev-l9-compatibility",
        "laravel/dusk": "*",
        "laravel/envoy": "^2.8",
        "laravel/pint": "^1.2",
        "laravel/telescope": "*",
        "nunomaduro/collision": "^6.1",
        "pestphp/pest": "^1.22",
        "pestphp/pest-plugin-laravel": "^1.3",
        "roquie/laravel-dusk-select2": "^2.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },

    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
